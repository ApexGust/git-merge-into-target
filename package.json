{
  "name": "git-merge-into-target",
  "displayName": "Git快速合并",
  "description": "一键将当前分支合并到目标分支并推送，避免频繁切换分支",
  "version": "1.0.0",
  "icon": "icon.png",
  "publisher": "prayfff",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/git-merge-into-target"
  },
  "keywords": [
    "git",
    "merge",
    "branch",
    "快速合并",
    "version control",
    "workflow"
  ],
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "activationEvents": [
    "onCommand:git-merge-into-target.quickMerge"
  ],
  "contributes": {
    "commands": [
      {
        "command": "gitMergeIntoTarget.mergeToTarget",
        "title": "Git: 快速合并到分支",
        "category": "Git"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "gitMergeIntoTarget.mergeToTarget",
          "when": "workspaceFolderCount > 0"
        }
      ]
    }
  },
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.60.0",
    "@types/node": "^16.x",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "simple-git": "^3.15.0"
  }
} 